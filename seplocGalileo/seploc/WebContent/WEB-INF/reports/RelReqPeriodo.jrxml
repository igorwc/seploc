	<subDataset name="GetNomeCliente">
		<parameter name="ClienteId" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[$P{REPORT_PARAMETERS_MAP}.get( "clientID" )]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
     tbl_clientes.`vcrRazao` AS tbl_clientes_vcrRazao
FROM
     `tbl_clientes` tbl_clientes
WHERE
     intClienteId = $P{ClienteId}]]>
		</queryString>
		<field name="vcrRazao" class="java.lang.String"/>
	</subDataset>
	<parameter name="clientID" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="dataInicio" class="java.util.Date">
		<defaultValueExpression><![CDATA[null]]></defaultValueExpression>
	</parameter>
	<parameter name="dataFim" class="java.util.Date">
		<defaultValueExpression><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select  count(rq.intNumreq) as qtdReq,c.vcrRazao, p.vcrProjeto as nomeProjeto,SUM(RQ.dblValorTotal) as Subtotal
from tbl_clientes c, tbl_reqserv rq, tbl_projetos p, tbl_statuscobranca sc
where c.intClienteId = p.intClienteId
and p.intCodProj = rq.intCodProj
and rq.intNumreq = sc.intNumreq
and c.intClienteId = $P{clientID}
and rq.datData > IFNULL($P{dataInicio}, '1900-01-01')
and rq.datData < $P{dataFim}
GROUP BY c.vcrRazao, p.vcrProjeto]]>
	</queryString>
	<field name="qtdReq" class="java.lang.Long"/>
	<field name="vcrRazao" class="java.lang.String"/>
	<field name="nomeProjeto" class="java.lang.String"/>
	<field name="Subtotal" class="java.lang.Double"/>
	<variable name="Total" class="java.lang.Double" calculation="Sum">
		<variableExpression><![CDATA[$F{Subtotal}.doubleValue()]]></variableExpression>
		<initialValueExpression><![CDATA[0.00]]></initialValueExpression>
	</variable>
