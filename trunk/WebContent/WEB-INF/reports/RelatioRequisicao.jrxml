		<queryString>
			<![CDATA[SELECT
     tbl_reqserv.`intNumreq` AS intNumreq,
     tbl_reqserv.`datData` AS datData,
     tbl_reqserv.`dblValorTotal` AS dblValorTotal,
     tbl_projetos.`intClienteId` AS intClienteId,
     tbl_projetos.`vcrProjeto` AS  vcrProjeto
